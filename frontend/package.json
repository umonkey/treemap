		"eslint-plugin-storybook": "^0.12.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"storybook": "^8.6.12",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.3.2",
		"vitest": "^2.0.4"
	},
